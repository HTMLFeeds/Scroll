{
	"compilerOptions": {
		"outFile": "build.js",
		"composite": true,
		"module": "system",
		"moduleResolution": "node",
		"target": "esnext",
		"inlineSourceMap": true,
		"inlineSources": true,
		"strict": true,
		"baseUrl": "./",
		"rootDir": ".",
		"declaration": true,
		"declarationMap": true,
		"stripInternal": true,
		"incremental": true,
		"preserveConstEnums": true,
		"lib": [
			"dom",
			"esnext",
			"esnext.array",
			"esnext.asynciterable"
		]
	},
	"include": [
		"build/**/*.ts",
		"../node_modules/fila-core/*.ts",
		"../node_modules/fila-node/*.ts"
	],
	"references": [
		{ "path": "../../Query" },
		{ "path": "../../Not" },
		{ "path": "../../Force" },
		{ "path": "../../Keyva" },
		{ "path": "../../Webfeed" },
		{ "path": "../../Dock" }
	]
}
