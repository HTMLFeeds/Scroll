{
	"$schema": "../../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeDevCommand": "",
		"beforeBuildCommand": "",
		"devPath": "../+build",
		"distDir": "../+bundle",
		"withGlobalTauri": true
	},
	"package": {
		"productName": "Rail",
		"version": "0.0.0"
	},
	"tauri": {
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "org.rail.app",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			]
		},
		"security": {
			"csp": "default-src *; img-src *"
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "Rail",
				"width": 600,
				"height": 800,
				"titleBarStyle": "Overlay"
			}
		]
	},
	"plugins": {
		"shell": {
			"open": true
		}
	}
}
